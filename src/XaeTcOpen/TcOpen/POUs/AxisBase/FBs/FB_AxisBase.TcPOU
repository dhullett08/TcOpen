    <Method Name="Enable" Id="{db2ed9b3-ee26-4fe1-966d-2deb43f58bd6}" FolderPath="Functions\">
      <Declaration><![CDATA[METHOD PRIVATE Enable : BOOL
]]></Declaration>
      <Implementation>
        <ST><![CDATA[//command pattern 
IF _enable THEN
	_power.Enable := TRUE;
	_asyncComd.SetBusy();
END_IF

IF _power.Status THEN
	_asyncComd.SetDone();
END_IF

IF _power.Error THEN
	_asyncComd.SetError();
END_IF]]></ST>
      </Implementation>
    </Method>
