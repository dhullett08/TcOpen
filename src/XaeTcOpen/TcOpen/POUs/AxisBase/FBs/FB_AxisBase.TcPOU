  <POU Name="FB_AxisBase" Id="{dbf7b8f1-ae81-4287-be87-eea1c81c44ed}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK FB_AxisBase IMPLEMENTS ITF_AxisBase
VAR_INPUT
END_VAR
VAR_OUTPUT
END_VAR
VAR
	_axis : AXIS_REF;
	_power : MC_Power;
	_enable : BOOL;
	
	_asyncComd : T_AsyncCommand;
	_cmdStatus : I_AsyncStatus := _asyncComd;
	
	_EnableBehaviour : T_Enable;
	_enabler : I_Enable := _EnableBehaviour;
END_VAR]]></Declaration>
    <Implementation>
      <ST><![CDATA[//command pattern code
Enable();
_power(Axis := _axis);

//strategy pattern code
//maybe move to an init method
_EnableBehaviour.RegisterAxis(axisRef := _axis);
_EnableBehaviour();

IF NOT _axis.Status.Disabled THEN
	Enabled := TRUE;
END_IF]]></ST>
    </Implementation>
