    <Method Name="EnableRequest" Id="{216b5385-5e56-4405-a3df-a11e0ff5af27}" FolderPath="Functions\">
      <Declaration><![CDATA[METHOD PUBLIC EnableRequest : I_AsyncStatus
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[//command pattern
IF Enabled THEN
	RETURN;
END_IF
_enable := TRUE;
EnableRequest := _cmdStatus;


//stategy pattern 
_enabler.Enable();]]></ST>
