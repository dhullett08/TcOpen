  <POU Name="FB_AnalogueInputBase" Id="{ffe48a9a-1d37-40f8-b7a7-45cd91910e96}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK FB_AnalogueInputBase IMPLEMENTS IAnalogueInput
VAR_INPUT
END_VAR
VAR_OUTPUT
END_VAR
VAR
	_processData : REFERENCE TO AnalogueStatus;
	_signalType : DINT;
	_resolution : DINT;
	_DataError : BOOL;
	
	dataPreviousToggle : BOOL;
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[IF dataPreviousToggle <> _processData.TxPDOToggle THEN
	dataPreviousToggle := _processData.TxPDOToggle;
	_DataError := FALSE;
ELSE
	_DataError := TRUE;
END_IF]]></ST>
    </Implementation>
    <Property Name="DeviceState" Id="{21cdaadc-48b4-420f-878f-23212c8fd49a}">
      <Declaration><![CDATA[PROPERTY PUBLIC DeviceState : bool]]></Declaration>
      <Get Name="Get" Id="{a1ee6aee-57b1-453e-bba8-3a3013143ba4}">
        <Declaration><![CDATA[VAR
END_VAR
]]></Declaration>
        <Implementation>
          <ST><![CDATA[DeviceState := _processData.TxPDOState;]]></ST>
        </Implementation>
      </Get>
    </Property>
    <Property Name="Error" Id="{79a5cfef-99b5-481c-81b7-13cb614e2233}">
      <Declaration><![CDATA[PROPERTY PUBLIC Error : bool]]></Declaration>
      <Get Name="Get" Id="{51c26d9b-c891-4b59-82f1-015c97e0525e}">
        <Declaration><![CDATA[VAR
END_VAR
]]></Declaration>
        <Implementation>
          <ST><![CDATA[Error := _processData.Underrange OR _processData.Overrange OR _processData.SyncError OR _processData.Error OR _DataError;]]></ST>
        </Implementation>
      </Get>
    </Property>
    <Method Name="FB_init" Id="{e0500f8c-eb79-4e42-bb25-72de298c7e14}">
      <Declaration><![CDATA[METHOD FB_init : BOOL
VAR_INPUT
	bInitRetains : BOOL; // if TRUE, the retain variables are initialized (warm start / cold start)
	bInCopyCode : BOOL;  // if TRUE, the instance afterwards gets moved into the copy code (online change)
	SignalType : AnalogueSignalType;
	Resolution : SignalResolution;
	ProcessData : reference to AnalogueStatus;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[CASE SignalType OF
	AnalogueSignalType._0_20mA :_signalType := 20;
	AnalogueSignalType._4_20mA : _signalType := 20;
	AnalogueSignalType._0_30V : _signalType := 30;
	AnalogueSignalType._10_10V : _signalType := 10;
	AnalogueSignalType._Invalid : _signalType := 0;
ELSE
	_signalType := 0;
END_CASE

CASE Resolution OF
	SignalResolution._12Bit : _resolution := 2048;
	SignalResolution._16Bit : _resolution := 32768;
	SignalResolution._24Bit : _resolution := 8388608;
	SignalResolution.Invalid : _resolution := 0;
ELSE
	_resolution := 0;
END_CASE

_processData := ProcessData;]]></ST>
      </Implementation>
    </Method>
    <Method Name="GetConvertedValue" Id="{1d77c5be-2386-4642-9531-4f10dc497ecd}">
      <Declaration><![CDATA[METHOD GetConvertedValue : REAL
VAR
END_VAR

]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF (_resolution = 0) OR (_signalType = 0) THEN
	RETURN;
ELSE
	GetConvertedValue := INT_TO_REAL(_processData.Value) * (DINT_TO_REAL(_signalType)/DINT_TO_REAL(_resolution));
END_IF
]]></ST>
      </Implementation>
    </Method>
    <Method Name="GetRawValue" Id="{840f64bd-c848-40ab-944f-b870f219b250}">
      <Declaration><![CDATA[METHOD GetRawValue : int
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[GetRawValue := _processData.Value;]]></ST>
      </Implementation>
    </Method>
    <Property Name="ValueLimit" Id="{1511724f-24ce-40d3-bd17-01a57b8c5b6c}">
