      <Get Name="Get" Id="{7933b0c8-a790-474a-8fd9-6a6caf3111dc}">
        <Declaration><![CDATA[VAR
END_VAR
]]></Declaration>
        <Implementation>
          <ST><![CDATA[ValueLimit := _processData.Limit1 OR _processData.Limit2;]]></ST>
        </Implementation>
      </Get>
    </Property>
